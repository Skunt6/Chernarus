            "CategoryName": "General",
            "Rules": [
                {
                    "RuleParagraph": "1.0.",
                    "RuleText": "Server resets at 12 and 6, AM and PM."
                },
                {
                    "RuleParagraph": "1.1.",
                    "RuleText": "Do not troll."
                },
                {
                    "RuleParagraph": "1.2.",
                    "RuleText": "Cheating will result in ban."
                },
                {
                    "RuleParagraph": "1.3.",
                    "RuleText": "Be resonable."
                },
                {
                    "RuleParagraph": "1.4.",
                    "RuleText": "PVE only."
                },
                {
                    "RuleParagraph": "1.5.",
                    "RuleText": "Do not touch other players stuff."
                },
                {
                    "RuleParagraph": "1.6.",
                    "RuleText": "Server is always being worked on. Any feedback comments or complaints please drop a message in feedback channel located on our Discord"
                },
                {
                    "RuleParagraph": "1.7.",
                    "RuleText": "There are mysteries located through out the map."
                },
                {
                    "RuleParagraph": "1.8.",
                    "RuleText": "When you die you have 1 hour to retieve your body, and whats on it before it despawns."
                }
            ]
        }
    ],
